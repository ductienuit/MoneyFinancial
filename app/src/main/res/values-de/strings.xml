    <string name="activity_intro_title_slide_1">Keine bösen Überraschungen mehr</string>
    <string name="activity_intro_description_slide_1">Hilft dir, all deine Ausgaben zu überwachen</string>
    <string name="activity_intro_title_slide_2">Entdecke und plane</string>
    <string name="activity_intro_description_slide_2">Du kannst herausfinden, wo du viel ausgibst und Budgets einrichten</string>
    <string name="activity_intro_title_slide_3">Quelloffen</string>
